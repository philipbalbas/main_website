{
	"name": "mauna_homepage",
	"version": "0.1.0",
	"scripts": {
		"dev": "concurrently 'npm run next:dev' 'npm run res:start'",
		"next:dev": "next",
		"debug": "NODE_OPTIONS='--inspect' next",
		"build": "bsb -clean-world -make-world && next build && next export",
		"now-build": "bsb -clean-world -make-world && next build",
		"export": "next export",
		"start": "next start -p $PORT",
		"res:build": "bsb -make-world",
		"res:clean": "bsb -clean-world",
		"res:start": "bsb -make-world -w"
	},
	"keywords": [
		"BuckleScript",
		"ReasonReact",
		"reason-react"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@chakra-ui/react": "^1.2.1",
		"@emotion/react": "^11.1.4",
		"@emotion/styled": "^11.0.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@fortawesome/pro-light-svg-icons": "^5.15.2",
		"@fortawesome/pro-solid-svg-icons": "^5.15.2",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"@headlessui/react": "^0.2.0",
		"@tailwindcss/forms": "^0.2.1",
		"@theme-ui/presets": "^0.3.5",
		"@zeit/next-css": "^1.0.1",
		"framer-motion": "3.3.0-beta.22",
		"next": "^10.0.6",
		"next-transpile-modules": "^4.1.0",
		"nookies": "^2.5.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"reason-react": "^0.9.1",
		"theme-ui": "^0.3.5",
		"typeface-roboto": "^0.0.75"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.20.0",
		"@types/react": "^17.0.0",
		"autoprefixer": "^10.2.1",
		"bs-platform": "^8.4.2",
		"concurrently": "^5.3.0",
		"cssnano": "^4.1.10",
		"gentype": "^3.41.0",
		"postcss": "^8.2.4",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "2.0.2",
		"typescript": "^4.1.3"
	}
}
